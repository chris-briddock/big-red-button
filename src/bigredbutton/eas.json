{
  "cli": {
    "version": ">= 16.17.4",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal"
    },
    "preview": {
      "distribution": "internal"
    },
    "production": {
      "autoIncrement": true
    }
  },
  "submit": {
    "production": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account-key.json",
        "track": "internal",
        "releaseStatus": "completed"
      }
    },
    "beta": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account-key.json",
        "track": "beta",
        "releaseStatus": "completed"
      }
    },
    "release": {
      "android": {
        "serviceAccountKeyPath": "./google-service-account-key.json",
        "track": "production",
        "releaseStatus": "completed"
      }
    }
  }
}
